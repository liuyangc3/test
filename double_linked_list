# web

class Node(object):
    def __init__(self, data, prev=None, next=None):
        self.data= data
        self.prev = prev
        self.next = next
        
class DoubleLinkedList(object):
    def __init__(self):
        self.count = 0  # numbers of nodes
        self.head = None  # first node
        self.tail = None  # last node
        
    def append(self, data):
        if self.head is None:
            self.head = self.tail = Node(data)
        else:
            node = Node(data, prev=self.tail)
            self.tail.next = node
            self.tail = node
        self.count += 1
        
    def pop(self):
        if self.count == 0:
            res = None
        elif self.count == 1:
            res = self.tail.data
            self.head = self.tail = None
        else:
            res = self.tail.data
            self.tail = self.tail.prev
            self.tail.next = None
        self.count -= 1
        return res

if __name__ == '__main__':
    dll = DoubleLinkedList()
    dll.append('fuck')
    dll.append('you')
    print(dll.count)
    print(dll.pop())
    print(dll.count)
    print(dll.pop())
    print(dll.count)  
